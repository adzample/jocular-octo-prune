.btn-file {
    position: relative;
    overflow: hidden;
    margin-left: 40vw;
    background-color #E5EBFA
    color #025C76
    border 2px solid #025C76
    font-size 22px
    padding 20px
    padding-left 40px
    padding-right 40px
}
.btn-file input[type=file] {
    position: absolute;
    top: 0;
    right: 0;
    min-width: 100%;
    min-height: 100%;
    font-size: 100px;
    text-align: right;
    opacity: 0;
    outline: none;
    background: white;
    cursor: inherit;
    display: block;
}
      
